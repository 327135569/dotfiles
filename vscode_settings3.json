{
    "workbench.editor.tabCloseButton": "off",
    "workbench.settings.editor": "json",
    "workbench.panel.defaultLocation": "bottom",
    "workbench.colorCustomizations": {
        "scrollbar.shadow": "#0000",
        "focusBorder": "#0000",
        "[Default Dark+]": {
            "panel.border": "#333",
            "tab.activeBackground": "#171717",
            "editor.lineHighlightBackground": "#555500",
            "editor.lineHighlightBorder": "#555500",
            "sideBarSectionHeader.background": "#171717"
        }
    },
    "markdown.preview.fontFamily": "\"Microsoft YaHei\", -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "terminal.integrated.fontFamily": "\"Iosevka Slab Regular\", \"Microsoft YaHei\", monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 500000,
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.env.windows": {
        "PYTHONIOENCODING": "UTF-8"
    },
    "editor.fontFamily": "\"Iosevka Slab Regular\", \"Microsoft YaHei\", monospace",
    "editor.fontSize": 14,
    "editor.renderLineHighlight": "gutter",
    "editor.renderIndentGuides": false,
    "editor.mouseWheelZoom": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.folding": false,
    "editor.suggest.showSnippets": false,
    "editor.selectionHighlight": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.trimAutoWhitespace": false,
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "comments": "#555",
            "textMateRules": [
                {
                    "scope": "support.function.builtin.python",
                    "settings": {
                        "foreground": "#98c379"
                    }
                }
            ]
        },
        "[Default Light+]": {
            "comments": "#ddd"
        }
    },
    "breadcrumbs.enabled": false,
    "breadcrumbs.filePath": "last",
    "breadcrumbs.icons": false,
    "explorer.openEditors.visible": 0,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "window.restoreWindows": "none",
    "window.enableMenuBarMnemonics": false,
    "window.zoomLevel": 0,
    "git.enabled": true,
    "git.decorations.enabled": false,
    "git.openDiffOnClick": true,
    "git.confirmEmptyCommits": true,
    "git.confirmForcePush": true,
    "git.confirmSync": true,
    "git.enableStatusBarSync": false,
    "git.allowForcePush": true,
    "git.suggestSmartCommit": false,
    "outline.problems.enabled": false,
    "outline.showVariables": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "debug.console.fontFamily": "\"Iosevka Slab Regular\", \"Microsoft YaHei\", monospace",
    "debug.console.fontSize": 13,
    "vim.statusBarColorControl": false,
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-x>": false,
        "<C-f>": false,
        "<C-a>": false,
        "<C-e>": false,
        "<C-p>": false
    },
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "D"
            ],
            "after": [
                "<C-d>"
            ]
        },
        {
            "before": [
                "U"
            ],
            "after": [
                "<C-u>"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "D"
            ],
            "after": [
                "<C-d>"
            ]
        },
        {
            "before": [
                "U"
            ],
            "after": [
                "<C-u>"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        }
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "diffEditor.renderIndicators": false,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.mypy_cache": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/__pycache__": true,
        "**/.gradle": true,
        "**/node_modules": true,
        "**/.DS_Store": true,
        "yarn-*.log": true,
        "yarn.lock": true
    },
    "http.proxy": "http://127.0.0.1:8118",
    "python.jediEnabled": true,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style={ based_on_style: pep8, column_limit: 120 }"
    ],
    "python.analysis.disabled": [
        "inherit-non-class"
    ],
    "python.linting.pylintArgs": [
        "--errors-only",
        "--disable=no-member"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.saturation": 1,
    "dart.debugExternalLibraries": true,
    "dart.debugSdkLibraries": true,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "gitlens.views.repositories.files.layout": "tree",
    "gitlens.views.repositories.avatars": false,
    "gitlens.views.fileHistory.avatars": false,
    "gitlens.views.lineHistory.avatars": false,
    "gitlens.hovers.avatars": false,
    "gitlens.sortBranchesBy": "date:desc",
    "gitlens.sortTagsBy": "date:desc",
    "gitlens.views.search.avatars": false,
    "gitlens.views.compare.avatars": false,
    "gitlens.blame.avatars": false,
    "gitlens.views.compare.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false
}
